{
  "name": "urgara-jornales-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "node scripts/prebuild.js",
    "build": "pnpm run prebuild && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "deploy": "npm run build && npm run pm2:start && npm run pm2:save",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate:all": "prisma generate --config=prisma-common/prisma.config.ts && prisma generate --config=prisma-locality/prisma.config.ts",
    "prisma:generate:common": "prisma generate --config=prisma-common/prisma.config.ts",
    "prisma:generate:locality": "prisma generate --config=prisma-locality/prisma.config.ts",
    "prisma:migrate:common": "prisma migrate dev --config=prisma-common/prisma.config.ts",
    "prisma:migrate:locality": "prisma migrate dev --config=prisma-locality/prisma.config.ts",
    "prisma:migrate:deploy:common": "prisma migrate deploy --config=prisma-common/prisma.config.ts",
    "prisma:migrate:deploy:locality": "prisma migrate deploy --config=prisma-locality/prisma.config.ts",
    "prisma:studio:common": "prisma studio --config=prisma-common/prisma.config.ts",
    "prisma:studio:locality": "prisma studio --config=prisma-locality/prisma.config.ts"
  },
  "dependencies": {
    "@nestjs/common": "11.1.6",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "11.1.6",
    "@nestjs/jwt": "11.0.0",
    "@nestjs/platform-express": "11.1.6",
    "@nestjs/schedule": "6.0.1",
    "@nestjs/swagger": "11.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "7.2.0",
    "@prisma/client-runtime-utils": "^7.2.0",
    "argon2": "0.43.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.2",
    "cookie-parser": "1.4.7",
    "csv-writer": "1.6.0",
    "fast-csv": "5.0.2",
    "pdfmake": "0.2.20",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.2",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.2.0",
    "@eslint/js": "9.18.0",
    "@nestjs/cli": "11.0.10",
    "@nestjs/schematics": "11.0.8",
    "@nestjs/testing": "11.1.6",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.5",
    "@types/argon2": "0.14.1",
    "@types/cookie-parser": "1.4.9",
    "@types/express": "5.0.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.16.0",
    "@types/pdfmake": "0.2.11",
    "@types/pg": "^8.16.0",
    "@types/supertest": "6.0.2",
    "@types/uuid": "10.0.0",
    "eslint": "9.39.2",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "5.2.2",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "prettier": "3.4.2",
    "source-map-support": "0.5.21",
    "supertest": "7.0.0",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.50.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
